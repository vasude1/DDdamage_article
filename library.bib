@article{Sharon1996a,
   abstract = {A rapidly moving crack in a brittle material is often idealized as a one-dimensional object with a singular tip, moving through a two-dimensional material. However, in real three-dimensional materials, tensile cracks form a planar surface whose edge is a rapidly moving one-dimensional singular front. The dynamics of these fronts under repetitive interaction with material inhomogeneities (asperities) and the morphology of the fracture surface that they create are not yet understood. Here we show that perturbations to a crack front in a brittle material result in long-lived and highly localized waves, which we call 'front waves' These waves exhibit a unique characteristic shape and propagate along the crack front at approximately the Rayleigh wave speed (the speed of sound along a free surface). Following interaction, counter-propagating front waves retain both their shape and amplitude. They create characteristic traces along the fracture surface, providing cracks with both inertia and a new mode of dissipation. Front waves are intrinsically three-dimensional, and cannot exist in conventional two-dimensional theories of fracture. Because front waves can transport and distribute asperity-induced energy fluctuations throughout the crack front, they may help to explain how cracks remain a single coherent entity, despite repeated interactions with randomly dispersed asperities.},
   author = {Eran Sharon and Jay Fineberg},
   doi = {10.1007/s11099-014-0033-x},
   issn = {03003604},
   issue = {10},
   journal = {Physical Review B},
   keywords = {NaCl,SPAD,Triticum aestivum L,biomass},
   pages = {7128-7139},
   pmid = {11242041},
   title = {Microbranching instability and the dynamic fracture of brittle materials},
   volume = {54},
   year = {1996}
}
@article{Bouchbinder2009,
   abstract = {A dynamic crack tip equation of motion is proposed based on the autonomy of the near-tip nonlinear zone of scale l(nl), symmetry principles, causality, and scaling arguments. Causality implies that the asymptotic linear-elastic fields at time t are determined by the crack path at a retarded time t-tau(d), where the delay time tau(d) scales with the ratio of l(nl) and the typical wave speed c(nl) within the nonlinear zone. The resulting equation is shown to agree with known results in the quasistatic regime. As a first application in the fully dynamic regime, an approximate analysis predicts a high-speed oscillatory instability whose characteristic scale is determined by l(nl). This prediction is corroborated by experimental results, demonstrating the emergence of crack tip inertialike effects.},
   author = {Eran Bouchbinder},
   doi = {10.1103/PhysRevLett.103.164301},
   issn = {00319007},
   issue = {16},
   journal = {Physical Review Letters},
   pages = {1-4},
   pmid = {19905697},
   title = {Dynamic crack tip equation of motion: High-speed oscillatory instability},
   volume = {103},
   year = {2009}
}
@article{Rosakis2002,
   abstract = {Recent experimental observations of intersonic shear\nrupture events that occur in a\nvariety of material systems have rekindled interest in the intersonic\nfailure phenomenon. Since the\nearly 1990s, engineers and scientists working in all length scales, from\nthe atomistic, the\nstructural, all the way up to the scale of the earth's deformation\nprocesses, have undertaken joint\nefforts to study this unexplored area of fracture mechanics. The\nanalysis in the present article\nemphasizes the cooperative and complementary manner in which\nexperimental observations and\nanalytical and numerical developments have proceeded. The article first\nreviews early contributions\nto the theoretical literature of dynamic subsonic and intersonic\nfracture and highlights the\nsignificant differences between tensile and shear cracks. The article\nthen uses direct laboratory\nobservations as a framework for discussing the physics of intersonic\nshear rupture occurring in\nconstitutively homogeneous (isotropic and anisotropic) as well as in\ninhomogeneous systems, all\ncontaining preferable crack paths or faults. Experiments, models, and\nfield evidence at a variety of\nlength scales (from the atomistic, the continuum, and up to the scale of\ngeological ruptures) are\nused to discuss processes such as (1) shock wave formation, (2)\nlarge-scale frictional contact and\nsliding at the rupture faces, and (3) maximum attainable rupture speeds\nand rupture speed stability.\nParticular emphasis is given to geophysical field evidence and to the\nexploration of the possibility\nof intersonic fault rupture during shallow crustal earthquake events.},
   author = {Ares J. Rosakis},
   doi = {10.1080/00018730210122328},
   isbn = {1617724106},
   issn = {00018732},
   issue = {4},
   journal = {Advances in Physics},
   pages = {1189-1257},
   pmid = {11991570},
   title = {Intersonic shear cracks and fault ruptures},
   volume = {51},
   year = {2002}
}
