@article{Morishita2016,
   abstract = {© 2015 Taylor  &  Francis Group, London, UK. Crack growth dynamics is studied experimentally and analyzed focusing on both kinetics and crack tip profiles utilizing acrylonitrile butadiene rubbers which are filled with different amounts of carbon black. The behavior of crack growth is characterized by the relation between the tearing energy and the crack growth rate, and all samples show the discontinuous transition of the crack growth rate between the ‘slow mode’ and ‘fast mode’ in a narrow range of the tearing energy. In the ‘fast mode’ crack growth rate (about 1–100 m/sec) shows a power-law relationship with the tearing energy, while in the ‘slow mode’ the crack growth rate (about 0.01–1 mm/sec) exhibits no definite dependence on the tearing energy. The high resolution measurements reveals that the features of the crack tip profile drastically change at the discontinuous transition of the crack growth rate. The crack tip profiles observed are characterized by the gap from the parabolic profile expected by Linear Elastic Fracture Mechanics (LEFM). The weakly nonlinear elasticity theory on fracture dynamics, which considers the contribution of the second-order modulus to the strain field, successfully explains the small gaps ( <  ca. 1 mm) independently of whether the mode of interestis ‘slow’ or ‘fast’. The failure of the theory becomes pronounced in the cases of the large gaps ( >  ca. 1 mm) in the ‘fast mode’ where the specimens undergo large strain and high strain rate. These results demonstrate the significance of the nonlinear viscoelasticity in the crack growth phenomena.},
   author = {Yoshihiro Morishita and Katsuhiko Tsunoda and Kenji Urayama},
   doi = {10.1103/PhysRevE.93.043001},
   isbn = {9781138028739},
   issn = {24700053},
   issue = {4},
   journal = {Physical Review E},
   pages = {1-11},
   title = {Velocity transition in the crack growth dynamics of filled elastomers: Contributions of nonlinear viscoelasticity},
   volume = {93},
   year = {2016}
}
@article{Morrissey1998,
   abstract = {We present simulations of 3 D dynamic fracture which suggest that a persistent elastic wave is generated in response to a localized perturbation of a propagating crack front, e.g., by a local heterogeneity of critical fracture energy. The wave propagates along the moving crack front and spreads, relative to its origin point on the fractured surface, at a speed slightly below the Rayleigh speed. The simulations were done using the spectral elastodynamic methodology of Geubelle and Rice (1995). They model failure by a displacement -weakening cohesive model, which corresponds in the singular crack limit to crack growth at a critical fracture energy. Confirmation that crack front waves with properties like in our simulation do exist has been provided by Ramanathan and Fisher (1997). Through a derivation based on the linearized perturbation analysis of dynamic singular tensile crack growth by Willis and Movchan (1995), those authors found by numerical evaluation that a transfer function thereby introduced has a simple pole at a certain ω/k ratio, corresponding to a non-dispersive wave. Further, we show that as a consequence of these persistent waves, when a crack grows through a region of small random fluctuations in fracture energy, the variances of both the local propagation velocity and the deformed slope of the crack front increase, according to linearized perturbation theory, in direct proportion to distance of growth into the randomly heterogeneous region. That rate of disordering is more rapid than the growth of the variances with the logarithm of distance established by Perrin and Rice (1994) for a model elastodynamic fracture theory based on a scalar wave equation. That scalar case, which shows slowly decaying (as t-1/2) rather than persistent crack front waves, is analyzed here too. © 1998 Elsevier Science Ltd. All rights reserved.},
   author = {John W Morrissey and James R Rice},
   doi = {10.1016/S0022-5096(97)00072-0},
   issn = {00225096},
   issue = {3},
   journal = {Journal of the Mechanics and Physics of Solids},
   keywords = {A. dynamic fracture,B. crack mechanics,B. stress waves,C. numerical algorithms},
   pages = {467-487},
   title = {Crack front waves},
   volume = {46},
   url = {http://esag.harvard.edu/rice/184_MorrisseyRice_JMPS_98.pdf},
   year = {1998}
}
