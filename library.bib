   author = {J. C. Simo},
   issn = {03002977},
   issue = {6},
   journal = {COMPUTER METHODS IN APPLIED MECHANICS AND ENGINEERING},
   pages = {198-200},
   title = {A FRAMEWORK FOR FINITE STRAIN ELASTOPLASTICITY BASED ON MAXIMUM PLASTIC DISSIPATION AND THE MULTIPLICATIVE DECOMPOSITION. PART II: COMPUTATIONAL ASPECTS},
   volume = {62},
