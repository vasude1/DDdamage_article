   publisher = {Elsevier Ltd},
   title = {Simulation of strain localization with discrete element-Cosserat continuum finite element two scale method for granular materials},
   volume = {122},
   url = {https://doi.org/10.1016/j.jmps.2018.09.029},
   year = {2019}
}
