   title = {New crack-tip elements for XFEM and applications to cohesive cracks},
   volume = {57},
   year = {2003}
}
@book{Willis1980a,
   abstract = {The overall properties of composites may be defined from the solution of some standard boundary value problem. For example, for elasticity, displacements consistent with a uniform strain throughout the composite may be prescribed on the boundary. The actual strain fluctuates but its mean takes the prescribed value and the object is to deduce the mean stress. For random composites, an ergodic hypothesis is invoked and the expectation value of the stress at some chosen point is sought instead. The problem so generated requires the solution of an integral equation. For a medium containing a dilute distribution of particles or fibres, expressions for the overall moduli as power series in the concentration c may be sought, by ensemble averaging the integral equation keeping one or more particles fixed. An infinite hierarchy of equations is generated in this way, which is solved by making an appropriate closure assumption, valid in the limit of low concentrations, at some stage: closing at the nth stage correctly estimates moduli up to terms of order cn. If such closure assumptions are made for high concentrations, they become no more than ad hoc approximations and yet, even the simplest, the “quasicrystalline approximation,” has been found to give estimates of overall moduli in agreement with those obtained from a “cell” approximation believed to generate reasonable results at any concentration. This is demonstrated via a formulation in terms of stress polarization and the associated variational principle, due to Hashin and Shtrikman. Allowance for higher order interactions is shown in many cases to yield bounds on the coefficients of c2 in the moduli and an example is worked out in detail.},
   author = {J.R. Willis},
   doi = {10.1016/B978-0-08-024728-1.50013-6},
   isbn = {9780080247281},
   journal = {Variational Methods in the Mechanics of Solids},
   pages = {59-66},
   title = {Variational Methods in the Mechanics of Solids},
   url = {http://www.sciencedirect.com/science/article/pii/B9780080247281500136},
   year = {1980}
}
@article{MooKoh1995,
   abstract = {This paper presents an incremental formulation of the moving-grid finite element method based on the Eulerian-Lagrangian kinematic description (ELD) to predict dynamic crack propagation in brittle materials. The variational statement of the equations of motion based on the ELD and an integral form of the energy balance criterion for crack growth are used to form the governing equations for the prediction problems of dynamic crack propagation. The resulting governing equations for the present formulation are nonlinear with respect to two unknowns, displacement and crack-tip position. An iterative solution scheme for the present formulation is proposed. The unknown crack-tip position is computed directly in the solution procedure. Numerical results of the prediction of mode I dynamic crack propagation are presented and compared with experimental results and other numerical solutions.},
   author = {Hyun Moo Koh and Hae Sung Lee and Un Yong Jeong},
   journal = {Nuclear Engineering and Design},
   pages = {295-309},
   title = {An incremental formulation of the moving-grid finite element method for the prediction of dynamic crack propagation},
   volume = {158},
   year = {1995}
}
@article{Brooks1982,
   abstract = {A new finite element formulation for convection dominated flows is developed. The basis of the formulation is the streamline upwind concept, which provides an accurate multidimensional generalization of optimal one-dimensional upwind schemes. When implemented as a consistent Petrov-Galerkin weighted residual method, it is shown that the new formulation is not subject to the artificial diffusion criticisms associated with many classical upwind methods. The accuracy of the streamline upwind/Petrov-Galerkin formulation for the linear advection diffusion equation is demonstrated on several numerical examples. The formulation is extended to the incompressible Navier-Stokes equations. An efficient implicit pressure/explicit velocity transient algorithm is developed which accomodates several treatments of the incompressibility constraint and allows for multiple iterations within a time step. The effectiveness of the algorithm is demonstrated on the problem of vortex shedding from a circular cylinder at a Reynolds number of 100. © 1982.},
   author = {Alexander N. Brooks and Thomas J.R. Hughes},
   doi = {10.1016/0045-7825(82)90071-8},
   isbn = {0045-7825},
   issn = {00457825},
   journal = {Computer Methods in Applied Mechanics and Engineering},
