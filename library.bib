@article{Cermelli1997a,
   abstract = {We propose a modification of Gurtin's (1994, 1995) approach to the theory of configurational forces, so as to account properly for inertia. Writing the configurational balance for an evolving part of the body in the form of a momentum balance, we arrive at representations of the configurational stress and the configurational momentum that coincide with the dynamic energy-momentum tensor and the quasi-momentum of Eshelby and also obtain a representation for the configurational momentum that coincides with the pseudo-momentum of Maugin and others. An advantage of our formulation is that it leads to an expression for the internal configurational force that, even when inertia is taken into account, includes no inertial contribution. Rather, that force consists solely of terms that reflect changes in material structure such as those that may accompany processes involving defect propagation, phase transitions and fracture.},
   author = {P. Cermelli and E. Fried},
   doi = {10.1098/rspa.1997.0103},
   issn = {13645021},
   issue = {1964},
   journal = {Proceedings of the Royal Society A: Mathematical, Physical and Engineering Sciences},
   pages = {1915-1927},
   title = {The influence of inertia on the configurational forces in a deformable solid},
   volume = {453},
   year = {1997}
}
@article{Atluri1984,
   abstract = {Certain incremental path-independent integrals, of relevance in the mechanics of fracture of elastic-plastic materials described by a classical flow theory of plasticity, are presented. Both quasi-static as well as dynamic fracture situations are considered. The topics discussed include: (i) incremental path-independent integrals that characterize the crack-tip fields in elastic-plastic materials; (ii) incremental integrals related to the incremental total potential energy difference; and (iii) the complementary or dual representations of these integrals. The use of these integrals is illustrated through some numerical examples. Comments are made on the utility of these integrals in postulating rational fracture criteria.},
